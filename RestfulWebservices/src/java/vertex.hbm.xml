<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.dg.model.VertexModel" table="VERTEX" >
        <id name="planet" type="string" column="PLANET_NODE" length="2" />
        <property name="planetName" type="string" column="PLANET_NAME" length="20" not-null="true"/>
    </class>

</hibernate-mapping>
